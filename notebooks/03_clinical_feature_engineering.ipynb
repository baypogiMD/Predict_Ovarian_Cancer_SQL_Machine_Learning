# Clinical Feature Engineering

import pandas as pd
import sqlite3
import numpy as np

conn = sqlite3.connect("data/database/ovarian_cancer.db")
df = pd.read_sql("SELECT * FROM model_dataset", conn)

# Feature engineering
df["age_menopause_interaction"] = df["age"] * df["menopause"]
df["log_ca125"] = np.log1p(df["ca125"])
df["log_he4"] = np.log1p(df["he4"])

